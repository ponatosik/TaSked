{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.Hosting.Lifetime": "Trace",
      "Microsoft.AspNetCore.Authentication": "Information"
    }
  },
  "Authentication": {
    "Schemes": {
      "Auth0": {
        "Authority": "https://tasked-app.eu.auth0.com",
        "ValidAudiences": [
          "https://tasked.com"
        ],
        "ValidIssuers": [
          "https://tasked-app.eu.auth0.com/"
        ]
      }
    }
  },
  "AllowedHosts": "*",
  "UseFirebaseCloudMessaging": "true"
}
